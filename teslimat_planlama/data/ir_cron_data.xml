<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Günlük Araç-İlçe Eşleştirme Kontrolü -->
    <record id="ir_cron_arac_ilce_sync" model="ir.cron">
        <field name="name">Teslimat: Araç-İlçe Eşleştirme Kontrolü</field>
        <field name="model_id" ref="model_teslimat_arac"/>
        <field name="state">code</field>
        <field name="code">model.sync_all_arac_ilce_eslesmesi()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True"/>
        <field name="doall" eval="False"/>
        <field name="priority">10</field>
    </record>
</odoo>
