<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Geçici View - Kaldırılacak -->
    <record id="view_teslimat_olustur_wizard_form" model="ir.ui.view">
        <field name="name">teslimat.olustur.wizard.form</field>
        <field name="model">teslimat.olustur.wizard</field>
        <field name="arch" type="xml">
            <form string="Geçici Wizard - Kaldırılacak">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                    <footer>
                        <button name="action_gecici" string="Geçici" type="object" class="btn-primary"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Geçici Action - Kaldırılacak -->
    <record id="action_teslimat_olustur_wizard" model="ir.actions.act_window">
        <field name="name">Geçici Wizard</field>
        <field name="res_model">teslimat.olustur.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>
